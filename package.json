  "license": "MIT",
  "scripts": {
    "test": "npx karma start karma.conf.js",
    "start": "webpack-dev-server --config build/webpack.config.dev.js",
    "build:dev": "webpack --config build/webpack.config.dist.dev.js",
    "build:min": "webpack --config build/webpack.config.dist.min.js"
  },
  "dependencies": {
    "@formulajs/formulajs": "^2.4.3",
    "antlr4": "^4.8.0",
    "chroma-js": "^2.1.0",
    "core-js-pure": "^3.6.5",
    "lodash": "^4.17.20",
    "lodash.debounce": "^4.0.8",
    "monaco-editor-core": "^0.20.0",
    "punycode": "^2.1.1"
  },
  "peerDependencies": {
    "monaco-editor-core": "^0.20.0"
  },
  "devDependencies": {
    "bufferutil": "^4.0.1",
    "cd": "^0.3.3",
    "chai": "^4.2.0",
    "coverage-istanbul-loader": "^3.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "gulp": "^4.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.1.1",
    "mochawesome": "^6.1.1",
    "nyc": "^15.1.0",
    "puppeteer": "^5.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "da": {
          "description": "Some new changes",
          "title": "Daily Development"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "docs": {
          "description": "Documentation only changes",
          "title": "Documentation"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
          "title": "Styles"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "perf": {
          "description": "A code change that improves performance",
          "title": "Performance Improvements"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "build": {
          "description": "Changes that affect the build system or external dependencies (example scopes: gulp, broccoli, npm)",
          "title": "Builds"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        }
      }
    }
  }
}
